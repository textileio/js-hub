{
  "name": "@textile/hub-filecoin",
  "version": "1.1.0",
  "description": "web-gRPC wrapper client for interacting with Textile Hub Filecoin services.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/!(*.spec).js?(.map)",
    "dist/**/!(*.spec).d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "test:browser": "polendina --cleanup --timeout 60 src/**/*.spec.ts --webpack-config ../../webpack.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-textile.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-textile/issues"
  },
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.12.0",
    "@textile/context": "^0.9.2",
    "@textile/crypto": "^2.0.0",
    "@textile/grpc-authentication": "^2.1.0",
    "@textile/grpc-connection": "^2.1.0",
    "@textile/grpc-powergate-client": "^1.2.4",
    "@textile/hub-grpc": "2.2.2",
    "@textile/security": "^0.6.2",
    "event-iterator": "^2.0.0",
    "loglevel": "^1.6.8"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "delay": "^4.3.0",
    "fast-sha256": "^1.3.0",
    "multibase": "^3.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  }
}